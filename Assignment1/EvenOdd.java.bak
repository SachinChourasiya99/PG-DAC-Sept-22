import java.util.Scanner;

class EvenOdd{
 public static void main(String[] args){
  Scanner a=new Scanner(System.in);
  System.out.println("Enter Number : ");
   int num = a.nextInt();
    
	if(num % 2 == 0){
	  System.out.println("Number is Even!");
	  System.out.println("Number is Even!");
	}
	else{
	  System.out.println("Number is Odd!");
	}
 
 }

}