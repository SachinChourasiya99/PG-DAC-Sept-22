import java.util.Scanner;

class Factorial{
 public static void main(String []args){
	int fact=1;
  Scanner num=new Scanner(System.in);
  int a = num.nextInt();
  System.out.println("Enter a Number For Factorial : "+a);
 
   for(int i=1;i<=a;i++){
       fact=fact*i;
    }
    System.out.println("Factorial of "+a+" is: "+fact);
   
   }
}